<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.order.mapper.OrderStoreSubListMapper">
    <select id="selectorderStoreListId" resultType="org.jeecg.modules.order.dto.OrderStoreSubListDTO">
        select * from  order_store_sub_list  where del_flag = 0
        <if test="orderStoreListId!=null and orderStoreListId!=''">
            and  order_store_list_id = #{orderStoreListId}
        </if>
        <if test="sysUserId!=null and sysUserId!=''">
            and  sys_user_id = #{sysUserId}
        </if>
        <if test="parentId!=null and parentId!=''">
            and  parent_id = #{parentId}
        </if>
        <if test="notParentId!=null and notParentId!=''">
            and  parent_id != #{notParentId}
        </if>
        ORDER BY create_time DESC
    </select>
</mapper>