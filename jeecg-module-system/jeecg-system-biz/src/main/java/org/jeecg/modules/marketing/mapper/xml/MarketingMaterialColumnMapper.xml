<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.marketing.mapper.MarketingMaterialColumnMapper">

    <select id="getMarketingMaterialColumnName" resultType="map">
        SELECT id,name FROM  `marketing_material_column` WHERE del_flag = 0 AND STATUS = 1 ORDER BY sort ASC ,create_time DESC
    </select>

</mapper>