<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.marketing.mapper.MarketingFreeGoodTypeMapper">

    <select id="getAllMarketingFreeGoodType" resultType="map">
              SELECT
          type_name AS typeName,
          id
        FROM
          `marketing_free_good_type`
        WHERE del_flag = '0'
          AND STATUS = '1'
        ORDER BY sort ASC

    </select>
</mapper>